package example1;

import udp.SocketUdp;

public class main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("APP SOCKET!");
		SocketUdp socket = new SocketUdp("localhost", 3000);
		socket.connect();
		socket.sendMsn("HOLA MUNDO");
		socket.disconnect();
	}

}
